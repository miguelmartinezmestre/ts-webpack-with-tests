    "jest": "latest",
    "prettier": "latest",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "latest",
    "typescript-declaration-webpack-plugin": "^0.2.2",
    "webpack": "latest",
